{
  "name": "fluido",
  "version": "1.0.2",
  "description": "A drop in replacement for stream with Promise support and more!",
  "type": "module",
  "main": "./fluido.cjs",
  "exports": {
    "import": "./fluido.mjs",
    "require": "./fluido.cjs"
  },
  "types": "./fluido.d.ts",
  "scripts": {
    "build": "rollup --config",
    "lint": "standard",
    "test": "c8 ava",
    "prepublishOnly": "npm run lint && npm run build && npm test"
  },
  "keywords": [
    "concurrency",
    "concurrent",
    "extra",
    "promise",
    "stream"
  ],
  "author": "Giacomo Gregoletto",
  "license": "MIT",
  "dependencies": {
    "readable-stream": "^4.0.0"
  },
  "devDependencies": {
    "ava": "^4.3.0",
    "c8": "^7.11.3",
    "rollup": "^2.75.6",
    "standard": "^17.0.0"
  },
  "files": [
    "!**/*.spec.mjs",
    "lib",
    "fluido.cjs",
    "fluido.d.ts",
    "fluido.mjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/greguz/fluido.git"
  },
  "homepage": "https://github.com/greguz/fluido"
}
