{
  "name": "fluido",
  "version": "1.0.2",
  "description": "A drop in replacement for stream with Promise support and more!",
  "main": "fluido.js",
  "types": "fluido.d.ts",
  "scripts": {
    "build": "rollup --config",
    "lint": "standard src",
    "test": "ava",
    "prepublishOnly": "npm run lint && npm run test"
  },
  "keywords": [
    "concurrency",
    "concurrent",
    "extra",
    "promise",
    "stream"
  ],
  "author": "Giacomo Gregoletto",
  "license": "MIT",
  "dependencies": {
    "readable-stream": "^3.6.0",
    "universalify": "^2.0.0"
  },
  "devDependencies": {
    "ava": "^4.0.1",
    "esm": "^3.2.25",
    "rollup": "^2.67.6",
    "standard": "^16.0.4"
  },
  "files": [
    "fluido.js",
    "fluido.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/greguz/fluido.git"
  },
  "homepage": "https://github.com/greguz/fluido",
  "ava": {
    "require": [
      "esm"
    ]
  }
}
