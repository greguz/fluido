{
  "name": "fluido",
  "version": "0.12.2",
  "description": "A modern streaming library",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "rollup --config",
    "test:lint": "eslint src test",
    "test:unit": "ava",
    "test": "npm run test:lint && npm run test:unit",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "stream",
    "toolkit",
    "readable",
    "writable",
    "duplex",
    "transform"
  ],
  "author": "Giacomo Gregoletto",
  "license": "MIT",
  "dependencies": {
    "end-of-stream": "^1.4.4",
    "pump": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "esm": "^3.2.25",
    "husky": "^3.0.8",
    "rollup": "^1.22.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/greguz/fluido.git"
  },
  "ava": {
    "require": [
      "esm"
    ]
